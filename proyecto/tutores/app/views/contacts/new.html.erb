<h<div class="row ">
<div class= "center col s12 m4 l10">
<h1 class="header teal-text text-lighten-2">ยก Holanda</h1>

<div class= "row">
	<%= form_for @contact do |f| %>
		<%= f.label :name %><br>
		<%= f.text_field :name, required: true %>

		<br>

		<%= f.label :email %><br>
		<%= f.email_field :email, required: true %>

		<br>

		<%= f.label :mensaje %><br>
		<%= f.text_area :message, as: :text %>

		<br>

		<%= f.submit 'Enviar mensaje', class: "button" %>

	<% end %>

	
</div>






