<p id="notice"><%= notice %></p>

<h1>Listing Ayudantes</h1>

<table>
  <thead>
    <tr>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Asignatura</th>
      <th>Carrera</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ayudantes.each do |ayudante| %>
      <tr>
        <td><%= ayudante.rut %></td>
        <td><%= ayudante.nombre %></td>
        <td><%= ayudante.apellido %></td>
        <td><%= ayudante.asignatura %></td>
        <td><%= ayudante.carrera %></td>
        <td><%= ayudante.email %></td>
        <td><%= link_to 'Show', ayudante %></td>
        <td><%= link_to 'Edit', edit_ayudante_path(ayudante) %></td>
        <td><%= link_to 'Destroy', ayudante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ayudante', new_ayudante_path %>
